<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:r="clr-namespace:Fluent;assembly=Fluent">
	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="pack://application:,,,/Fluent;Component/Themes/Office2010/Colors/ColorsSilver.xaml" />
		<ResourceDictionary Source="pack://application:,,,/Fluent;component/Themes/Office2010/Colors/Colors.xaml"/>
		<ResourceDictionary Source="pack://application:,,,/Fluent;component/Themes/Office2010/Controls/BackstageTabItem.xaml"/>
		<ResourceDictionary Source="pack://application:,,,/Fluent;component/Themes/Office2010/Controls/BackstageTabControl.xaml"/>
		<!--<ResourceDictionary Source="pack://application:,,,/Fluent;component\Themes\Office2010\Controls\StatusBar.xaml"/>-->
		<ResourceDictionary Source="pack://application:,,,/Fluent;component\Themes\Office2010\Controls\RibbonStatusBar.xaml"/>
	</ResourceDictionary.MergedDictionaries>

	<Style x:Key="{x:Type r:BackstageTabControl}" TargetType="{x:Type r:BackstageTabControl}" BasedOn="{StaticResource BackstageStyle}"/>
	<Style x:Key="{x:Type r:BackstageTabItem}" TargetType="{x:Type r:BackstageTabItem}" BasedOn="{StaticResource BackstageTabItemStyle}"/>
</ResourceDictionary>