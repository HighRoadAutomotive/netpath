					<RowDefinition Height="25" />
					<RowDefinition Height="*" />
				</Grid.RowDefinitions>
				<Border BorderThickness="0,0,0,1" BorderBrush="{StaticResource GridLineBrush}" >
					<Grid>
						<Grid.ColumnDefinitions>
