		<ToggleButton Content="No" Grid.Column="1" Grid.Row="1" Height="23" HorizontalAlignment="Right" IsChecked="{Binding Path=Data.IsFlags}" Margin="0,6,5,0" Name="IsFlags" VerticalAlignment="Top" Width="100" IsHitTestVisible="{Binding Path=Data, Converter={StaticResource ProjectTypeEnabledConverter}}" Checked="IsFlags_Checked" Unchecked="IsFlags_Unchecked"/>
		<Label Content="Is Reference:" Height="Auto" HorizontalAlignment="Left" Margin="5,5,0,0" VerticalAlignment="Top" Width="Auto" Grid.Column="2" Grid.Row="1" />
		<ToggleButton Content="No" Grid.Column="2" Grid.Row="1" Height="23" HorizontalAlignment="Right" IsChecked="{Binding Path=Data.IsReference}" Margin="0,6,5,0" Name="IsReference" VerticalAlignment="Top" Width="100" IsHitTestVisible="{Binding Path=Data, Converter={StaticResource ProjectTypeEnabledConverter}}" Checked="IsReference_Checked" Unchecked="IsReference_Unchecked"/>
	</Grid>
	<GroupBox Grid.Column="0" Header="Documentation" Margin="2,0,5,0" Visibility="Collapsed" Grid.Row="1" >
		<Grid>
		</Grid>
	</GroupBox>
	<Grid Name="Values" Grid.Column="0" Grid.Row="2" >
