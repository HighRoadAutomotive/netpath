								<Button Padding="5,0" Style="{StaticResource GridHeaderButtonStyle}" HorizontalContentAlignment="Stretch" Content="Value" Name="ValueFilterBarValue" Click="ValueFilterBarValue_Click" />
							</Grid>
						</Border>
						<Border Grid.Column="2" BorderThickness="0,0,1,0" BorderBrush="{StaticResource GridLineBrush}" >
							<Grid>
								<pc:SearchTextBox LabelText="Filter Contract Value" LabelTextColor="Gray" Name="ValueFilterContractBox" Margin="0" BorderThickness="1" CornerRadius="0" TextChanged="ValueFilterContractBox_TextChanged" LostFocus="ValueFilterContractBox_LostFocus" />
								<Button Padding="5,0" Style="{StaticResource GridHeaderButtonStyle}" HorizontalContentAlignment="Stretch" Content="Contract Value" Name="ValueFilterBarContract" Click="ValueFilterBarContract_Click" />
							</Grid>
						</Border>
						<Border Grid.Column="3" BorderThickness="0,0,1,0" BorderBrush="{StaticResource GridLineBrush}" >
							<Border Padding="5,0" Background="{StaticResource GridHeaderButtonNormalBrush}" >
								<TextBlock Text="Exclude" HorizontalAlignment="Left" VerticalAlignment="Center" />
							</Border>
						</Border>
						<Border Grid.Column="4" BorderThickness="0,0,1,0" BorderBrush="{StaticResource GridLineBrush}" >
							<Border Padding="5,0" Background="{StaticResource GridHeaderButtonNormalBrush}" >
								<TextBlock Text="Hidden" HorizontalAlignment="Left" VerticalAlignment="Center" />
							</Border>
						</Border>
						<Border Grid.Column="5" BorderThickness="0,0,1,0" BorderBrush="{StaticResource GridLineBrush}" Background="{StaticResource GridHeaderButtonNormalBrush}" />
						<Border Grid.Column="6" BorderThickness="0,0,1,0" BorderBrush="{StaticResource GridLineBrush}" Background="{StaticResource GridHeaderButtonNormalBrush}" />
						<Border Grid.Column="7" BorderThickness="0,0,1,0" BorderBrush="{StaticResource GridLineBrush}" Background="{StaticResource GridHeaderButtonNormalBrush}" />
					</Grid>
				</Border>
				<ListBox Grid.Row="1" Name="ValuesList" AllowDrop="True" BorderThickness="0" Margin="0" Padding="-1" SelectionMode="Single" ScrollViewer.HorizontalScrollBarVisibility="Disabled" ScrollViewer.VerticalScrollBarVisibility="Visible" VirtualizingStackPanel.IsVirtualizing="False" ItemContainerStyle="{StaticResource StretchedValueListItemContainerStyle}" PreviewMouseLeftButtonDown="ValuesList_PreviewMouseLeftButtonDown" PreviewMouseMove="ValuesList_PreviewMouseMove" QueryContinueDrag="ValuesList_QueryContinueDrag" DragLeave="ValuesList_DragLeave" PreviewDragOver="ValuesList_PreviewDragOver" PreviewDrop="ValuesList_Drop" >
					<ListBox.Resources>
						<SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="#00FFFFFF"/>
					</ListBox.Resources>
					<ListBox.ItemTemplate>
						<DataTemplate>
							<Border Name="BG" Background="Transparent" BorderBrush="{StaticResource GridLineBrush}" BorderThickness="0,0,0,1" Margin="0" IsEnabled="{Binding Path=., Converter={StaticResource ProjectTypeEnabledConverter}}">
								<Grid>
									<Grid.RowDefinitions>
										<RowDefinition Height="27" />
									</Grid.RowDefinitions>
									<Grid.ColumnDefinitions>
										<ColumnDefinition Width="{Binding ElementName=ItemsGridCD1, Path=Width}" />
										<ColumnDefinition Width="{Binding ElementName=ItemsGridCD2, Path=Width}" />
										<ColumnDefinition Width="{Binding ElementName=ItemsGridCD3, Path=Width}" />
										<ColumnDefinition Width="55" />
										<ColumnDefinition Width="55" />
										<ColumnDefinition Width="75" />
										<ColumnDefinition Width="30" />
									</Grid.ColumnDefinitions>
									<Border Grid.Column="0" BorderThickness="0,0,1,0" BorderBrush="{StaticResource GridLineBrush}" >
										<pc:TextBox Name="ValueName" Text="{Binding Path=Name}" VerticalContentAlignment="Center" BorderThickness="1" Background="Transparent" CornerRadius="0" IsHaloEnabled="False" IsReadOnly="{Binding Path=., Converter={StaticResource ProjectTypeReadOnlyConverter}}" PreviewKeyDown="ValueName_PreviewKeyDown" GotFocus="ValueName_GotFocus" LostFocus="ValueName_LostFocus" />
									</Border>
									<Border Grid.Column="1" BorderThickness="0,0,1,0" BorderBrush="{StaticResource GridLineBrush}" >
										<pc:TextBox Name="Value" Text="{Binding Path=Value}" VerticalContentAlignment="Center" BorderThickness="1" Background="Transparent" CornerRadius="0" IsHaloEnabled="False" IsReadOnly="{Binding Path=., Converter={StaticResource ProjectTypeReadOnlyConverter}}" PreviewKeyDown="Value_PreviewKeyDown" GotFocus="Value_GotFocus" LostFocus="Value_LostFocus" Validate="Value_Validate" />
									</Border>
									<Border Grid.Column="2" BorderThickness="0,0,1,0" BorderBrush="{StaticResource GridLineBrush}" >
										<pc:TextBox Name="ContractValue" Text="{Binding Path=ContractValue}" VerticalContentAlignment="Center" BorderThickness="1" Background="Transparent" CornerRadius="0" IsHaloEnabled="False" IsReadOnly="{Binding Path=., Converter={StaticResource ProjectTypeReadOnlyConverter}}" PreviewKeyDown="ContractValue_PreviewKeyDown" GotFocus="ContractValue_GotFocus" LostFocus="ContractValue_LostFocus" Validate="ContractValue_Validate" />
									</Border>
									<Border Grid.Column="3" BorderThickness="0,0,1,0" BorderBrush="{StaticResource GridLineBrush}" >
										<CheckBox IsChecked="{Binding Path=IsExcluded}" HorizontalAlignment="Center" VerticalAlignment="Center" IsHitTestVisible="{Binding Path=., Converter={StaticResource ProjectTypeEnabledConverter}}" />
									</Border>
									<Border Grid.Column="4" BorderThickness="0,0,1,0" BorderBrush="{StaticResource GridLineBrush}" >
										<CheckBox IsChecked="{Binding Path=IsHidden}" HorizontalAlignment="Center" VerticalAlignment="Center" IsHitTestVisible="{Binding Path=., Converter={StaticResource ProjectTypeEnabledConverter}}" />
									</Border>
									<Border Grid.Column="5" BorderThickness="0,0,1,0" BorderBrush="{StaticResource GridLineBrush}" IsHitTestVisible="False" >
										<Label Content="Click to Move" FontSize="10" FontStyle="Italic" HorizontalAlignment="Center" VerticalAlignment="Center" IsHitTestVisible="False" />
									</Border>
									<Button Name="DeleteValue" Visibility="Hidden" Grid.Column="6" Margin="1,1,2,1" Padding="0" Style="{StaticResource ListItemButtonStyle}" IsHitTestVisible="{Binding Path=., Converter={StaticResource ProjectTypeEnabledConverter}}" Click="DeleteValue_Click">
										<Image Source="pack://application:,,,/WCFArchitect;component/Icons/X16/Delete.png" HorizontalAlignment="Center" VerticalAlignment="Center" UseLayoutRounding="True" Height="16" Width="16" Stretch="Uniform"/>
									</Button>
								</Grid>
							</Border>
							<DataTemplate.Triggers>
								<Trigger SourceName="ValueName" Property="IsKeyboardFocusWithin" Value="True">
									<Setter TargetName="ValueName" Property="Background" Value="White" />
								</Trigger>
								<Trigger SourceName="Value" Property="IsKeyboardFocusWithin" Value="True">
									<Setter TargetName="Value" Property="Background" Value="White" />
								</Trigger>
								<Trigger SourceName="ContractValue" Property="IsKeyboardFocusWithin" Value="True">
									<Setter TargetName="ContractValue" Property="Background" Value="White" />
								</Trigger>
								<MultiDataTrigger>
									<MultiDataTrigger.Conditions>
										<Condition Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor,AncestorType={x:Type ListBoxItem}},Path=IsMouseOver}" Value="True" />
										<Condition Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor,AncestorType={x:Type ListBoxItem}},Path=IsSelected}" Value="False" />
									</MultiDataTrigger.Conditions>
									<Setter TargetName="BG" Property="Background" Value="{StaticResource GridLineMouseOverBackground}" />
									<Setter TargetName="DeleteValue" Property="Visibility" Value="Visible" />
								</MultiDataTrigger>
								<MultiDataTrigger>
									<MultiDataTrigger.Conditions>
										<Condition Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor,AncestorType={x:Type ListBoxItem}},Path=IsSelected}" Value="True" />
										<Condition Binding="{Binding Path=., Converter={StaticResource ProjectTypeEnabledConverter}}" Value="True" />
									</MultiDataTrigger.Conditions>
									<Setter TargetName="BG" Property="Background" Value="White" />
									<Setter TargetName="DeleteValue" Property="Visibility" Value="Visible" />
								</MultiDataTrigger>
								<MultiDataTrigger>
									<MultiDataTrigger.Conditions>
										<Condition Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor,AncestorType={x:Type ListBoxItem}},Path=IsSelected}" Value="True" />
										<Condition Binding="{Binding Path=., Converter={StaticResource ProjectTypeEnabledConverter}}" Value="False" />
									</MultiDataTrigger.Conditions>
									<Setter TargetName="BG" Property="Background" Value="White" />
								</MultiDataTrigger>
							</DataTemplate.Triggers>
						</DataTemplate>
					</ListBox.ItemTemplate>
				</ListBox>
			</Grid>
		</Border>
	</Grid>
