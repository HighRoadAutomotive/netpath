<Grid x:Class="WCFArchitect.Interface.Dialogs.NewProject"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        MinHeight="75" MinWidth="600">
	<Label Content="Name:" HorizontalAlignment="Left" Margin="10,11,0,0" VerticalAlignment="Top" />
	<TextBox x:Name="NewProjectName" Height="23" Margin="90,12,10,0" VerticalAlignment="Top" />
	<Label Content="Location:" HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" />
	<TextBox x:Name="NewProjectLocation" Height="23" Margin="90,41,91,0" VerticalAlignment="Top" />
	<Button x:Name="NewProjectBrowse" Content="Browse..." Height="23" HorizontalAlignment="Right" Margin="0,41,10,0" VerticalAlignment="Top" Width="75" Click="NewProjectBrowse_Click" />
</Grid>