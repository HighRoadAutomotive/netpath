			<TextBlock Name="ItemTitle" Grid.Column="1" Text="{Binding Path=Name}" Padding="3,0" VerticalAlignment="Center" TextTrimming="CharacterEllipsis" TextWrapping="NoWrap" />
			<Grid.ContextMenu>
				<ContextMenu Visibility="{Binding Path=IsDependencyProject, Converter={StaticResource SolutionNavigatorContextMenuVisibilityConverter}}">
