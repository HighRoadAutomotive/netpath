<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:Root="clr-namespace:WCFArchitect.Interface"
					xmlns:Projects="clr-namespace:WCFArchitect.Projects"
					xmlns:Helper="clr-namespace:WCFArchitect.Themes">

	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="pack://application:,,,/WCFArchitect;component/Themes/Generic.xaml" />
	</ResourceDictionary.MergedDictionaries>

	<HierarchicalDataTemplate x:Key="BindingsHeaderTemplate" ItemsSource="{Binding Path=.}">
		<Grid>
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="20" />
				<ColumnDefinition Width="*" />
			</Grid.ColumnDefinitions>
			<Image Name="ItemImage" Source="pack://application:,,,/WCFArchitect;component/Icons/X16/Binding.png" HorizontalAlignment="Right" VerticalAlignment="Center" UseLayoutRounding="True" Height="16" Width="16" Margin="0,1" Stretch="Uniform" Visibility="Visible" Grid.Column="0"/>
			<TextBlock Name="ItemTitle" Grid.Column="1" Text="Bindings" Padding="3,0" VerticalAlignment="Center" TextTrimming="CharacterEllipsis" TextWrapping="NoWrap" />
		</Grid>
	</HierarchicalDataTemplate>
	<HierarchicalDataTemplate x:Key="SecurityHeaderTemplate" ItemsSource="{Binding Path=.}">
		<Grid>
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="20" />
				<ColumnDefinition Width="*" />
			</Grid.ColumnDefinitions>
			<Image Name="ItemImage" Source="pack://application:,,,/WCFArchitect;component/Icons/X16/BindingSecurity.png" HorizontalAlignment="Right" VerticalAlignment="Center" UseLayoutRounding="True" Height="16" Width="16" Margin="0,1" Stretch="Uniform" Visibility="Visible" Grid.Column="0"/>
			<TextBlock Name="ItemTitle" Grid.Column="1" Text="Security" Padding="3,0" VerticalAlignment="Center" TextTrimming="CharacterEllipsis" TextWrapping="NoWrap" />
		</Grid>
	</HierarchicalDataTemplate>
	<HierarchicalDataTemplate x:Key="HostsHeaderTemplate" ItemsSource="{Binding Path=.}">
		<Grid>
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="20" />
				<ColumnDefinition Width="*" />
			</Grid.ColumnDefinitions>
			<Image Name="ItemImage" Source="pack://application:,,,/WCFArchitect;component/Icons/X16/Host.png" HorizontalAlignment="Right" VerticalAlignment="Center" UseLayoutRounding="True" Height="16" Width="16" Margin="0,1" Stretch="Uniform" Visibility="Visible" Grid.Column="0"/>
			<TextBlock Name="ItemTitle" Grid.Column="1" Text="Hosts" Padding="3,0" VerticalAlignment="Center" TextTrimming="CharacterEllipsis" TextWrapping="NoWrap" />
		</Grid>
	</HierarchicalDataTemplate>

	<Root:ProjectNavigatorTemplateSelector BindingsHeader="{StaticResource BindingsHeaderTemplate}" SecurityHeader="{StaticResource SecurityHeaderTemplate}" HostsHeader="{StaticResource HostsHeaderTemplate}" x:Key="SolutionNavigatorTemplateSelector" />
	<Root:ProjectNavigatorMultiBinding x:Key="ProjectNavigatorMultiBinding" />
	<Root:SolutionNavigatorContextMenuVisibilityConverter x:Key="SolutionNavigatorContextMenuVisibilityConverter" />

	<HierarchicalDataTemplate DataType="{x:Type Projects:Project}">
		<HierarchicalDataTemplate.ItemsSource>
			<MultiBinding Converter="{StaticResource ProjectNavigatorMultiBinding}">
				<Binding Path="Bindings" />
				<Binding Path="Security" />
				<Binding Path="Hosts" />
				<Binding Path="Namespaces" />
				<Binding Path="Services" />
				<Binding Path="Data" />
				<Binding Path="Enums" />
			</MultiBinding>
		</HierarchicalDataTemplate.ItemsSource>
		<Grid IsHitTestVisible="True">
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="20" />
				<ColumnDefinition Width="*" />
			</Grid.ColumnDefinitions>
			<Image Name="ItemImage" Source="pack://application:,,,/WCFArchitect;component/Icons/X16/Project.png" HorizontalAlignment="Right" VerticalAlignment="Center" UseLayoutRounding="True" Height="16" Width="16" Margin="0,1" Stretch="Uniform" Visibility="Visible" Grid.Column="0"/>
			<TextBlock Name="ItemTitle" Grid.Column="1" Text="{Binding Path=Name}" Padding="3,0" VerticalAlignment="Center" TextTrimming="CharacterEllipsis" TextWrapping="NoWrap" />
			<Grid.ContextMenu>
				<ContextMenu>
					<MenuItem Header="Build" Icon="pack://application:,,,/WCFArchitect;component/Icons/X16/BuildProject.png" Command="{x:Static Root:Main.BuildProjectCommand}" CommandParameter="{Binding}" />
					<MenuItem Header="Output" Icon="pack://application:,,,/WCFArchitect;component/Icons/X16/BuildOutput.png" Command="{x:Static Root:Main.OutputProjectCommand}" CommandParameter="{Binding}" />
					<Separator Style="{StaticResource SeperatorStyle}" />
					<MenuItem Header="Add..." Icon="pack://application:,,,/WCFArchitect;component/Icons/X16/Add.png" Command="{x:Static Root:Main.AddItemCommand}" CommandParameter="{Binding}" />
					<Separator Style="{StaticResource SeperatorStyle}" />
					<MenuItem Header="Add Binding" Icon="pack://application:,,,/WCFArchitect;component/Icons/X16/Binding.png" Command="{x:Static Root:Main.AddBindingCommand}" CommandParameter="{Binding}" />
					<MenuItem Header="Add Security" Icon="pack://application:,,,/WCFArchitect;component/Icons/X16/BindingSecurity.png" Command="{x:Static Root:Main.AddSecurityCommand}" CommandParameter="{Binding}" />
					<MenuItem Header="Add Host" Icon="pack://application:,,,/WCFArchitect;component/Icons/X16/Host.png" Command="{x:Static Root:Main.AddHostCommand}" CommandParameter="{Binding}" />
					<Separator Style="{StaticResource SeperatorStyle}" />
					<MenuItem Header="Add Namespace" Icon="pack://application:,,,/WCFArchitect;component/Icons/X16/Namespace.png" Command="{x:Static Root:Main.AddNamespaceCommand}" CommandParameter="{Binding}" />
					<Separator Style="{StaticResource SeperatorStyle}" />
					<MenuItem Header="Add Service" Icon="pack://application:,,,/WCFArchitect;component/Icons/X16/Service.png" Command="{x:Static Root:Main.AddServiceCommand}" CommandParameter="{Binding}" />
					<MenuItem Header="Add Data" Icon="pack://application:,,,/WCFArchitect;component/Icons/X16/Data.png" Command="{x:Static Root:Main.AddDataCommand}" CommandParameter="{Binding}" />
					<MenuItem Header="Add Enum" Icon="pack://application:,,,/WCFArchitect;component/Icons/X16/Enum.png" Command="{x:Static Root:Main.AddEnumCommand}" CommandParameter="{Binding}" />
					<Separator Style="{StaticResource SeperatorStyle}" />
					<MenuItem Header="Delete" Icon="pack://application:,,,/WCFArchitect;component/Icons/X16/Delete.png" Command="{x:Static Root:Main.DeleteItemCommand}" CommandParameter="{Binding}" />
				</ContextMenu>
			</Grid.ContextMenu>
		</Grid>
	</HierarchicalDataTemplate>
